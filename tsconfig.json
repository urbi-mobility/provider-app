{
  "compilerOptions": {
    "target": "es2017",
    "module": "es2015",
    "outDir": "build/dist",
    "sourceMap": true,
    "jsx": "react",
    "lib": ["dom", "es2017"],
    "moduleResolution": "node",
    "strict": true,
    "paths": {
      "provider-app/*": ["*", "*.ios", "*.android", "*.json"],
      "Urbi/*": [
        "./Urbi/*",
        "./Urbi/*.ios",
        "./Urbi/*.android",
        "./Urbi/*.json"
      ]
    },
    "baseUrl": "."
  }
}

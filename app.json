{
  "expo": {
    "name": "provider-app",
    "slug": "BobCars",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["urbiwallet"]
      }
    },
    "android": {
      "package": "com.bobcars"
    },
    "scheme": "bobcars",
    "description": "A mock mobility provider app"
  }
}
